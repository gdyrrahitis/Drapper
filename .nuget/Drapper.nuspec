    <releaseNotes>First release (production ready).</releaseNotes>
    <copyright>Copyright 2016</copyright>
    <dependencies>
      <dependency id="Drapper.Validation" version="1.0.0" />
      <dependency id="Dapper" version="1.50.2" />
      <dependency id="Newtonsoft.Json" version="9.0.1" />
    </dependencies>
  </metadata>
  <files>
    <file src="net452\Drapper.dll" target="lib\net452\Drapper.dll"/>    
    <file src="net461\Drapper.dll" target="lib\net461\Drapper.dll"/>    
    <file src="content\Drapper.Settings.json" target="content\Drapper.Settings.json"/>
  </files>
</package>